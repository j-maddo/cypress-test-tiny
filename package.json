{
  "name": "cypress-test-tiny",
  "version": "1.0.0",
  "description": "Small project for quickly testing Cypress.io test runner",
  "scripts": {
    "cypress:run": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress run",
    "cypress:open": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress open",
    "cypress:verify": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress verify",
    "cypress:version": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress version",
    "cypress:info": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress info",
    "cypress:cache:list": "CYPRESS_CACHE_FOLDER=.cy-cache cypress install && cypress cache list",
    "list-videos": "ls -l cypress/videos",
    "check-videos": "ok-file 'cypress/videos/*.mp4'",
    "set-status": "set-status"
  },
  "keywords": [],
  "author": "Gleb Bahmutov <gleb@cypress.io>",
  "license": "ISC",
  "devDependencies": {
    "cypress": "^12.3",
    "ok-file": "1.5.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/cypress-io/cypress-test-tiny.git"
  },
  "packageManager": "yarn@3.1.0"
}
